# Example environment variables for EM Gurus
# DO NOT COMMIT SECRETS. Fill these in Supabase → Project Settings → Functions → Secrets

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL_CHAT_BASIC=gpt-5.0-nano
OPENAI_MODEL_CHAT_EXAM=gpt-5.0-pro
OPENAI_MODEL_EXAM=gpt-5.0-pro
OPENAI_EMBED_MODEL=text-embedding-3-small
OPENAI_EMBED_DIM=1536
OPENAI_CHAT_TIMEOUT_MS=60000
OPENAI_EMBED_TIMEOUT_MS=30000
CHAT_MAX_TOKENS=300
EXAM_MAX_TOKENS=800

# CORS allowlist (comma-separated). Use * to allow all in dev only.
ORIGIN_ALLOWLIST=https://emgurus.com,https://www.emgurus.com,http://localhost:3000

# Supabase project
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Other (optional)
FIRECRAWL_API_KEY=

# Frontend (optional)
VITE_FUNCTIONS_BASE_URL=https://<PROJECT_REF>.functions.supabase.co
